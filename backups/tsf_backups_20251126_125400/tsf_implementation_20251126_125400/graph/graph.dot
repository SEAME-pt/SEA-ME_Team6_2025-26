// Minimal TSF graph for Sea:ME example
digraph TSF {
  // nodes refer to items by UID
  "EX-001" [label="Expectation EX-001: Latency <100ms"];
  "AS-001" [label="Assertion AS-001: Telemetry 10Hz, jitter <20ms"];
  "ASU-001" [label="Assumption ASU-001: Network latency <30ms"];
  "EV-001" [label="Evidence EV-001: Perf tests 2025-11-25"];

  // Links: parent -> child (A implies B: parent -> child)
  // EX-001 depends on AS-001 (AS-001 supports EX-001)
  "EX-001" -> "AS-001" [label="supported-by"];

  // AS-001 depends on ASU-001 (assumption) and EV-001 (evidence)
  "AS-001" -> "ASU-001" [label="requires"];
  "AS-001" -> "EV-001" [label="evidence"];
}
